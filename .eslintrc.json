{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "google",
        "plugin:react/recommended",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "rules": {
        "@typescript-eslint/no-unused-vars": ["error", {
            "args": "none"
        }],
        "no-invalid-this": "warn"
    }
}
